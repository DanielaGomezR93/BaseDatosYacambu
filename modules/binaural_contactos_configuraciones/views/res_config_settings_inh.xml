<odoo>
  <data>
	  <record id="res_config_settings_inherit_view_form" model="ir.ui.view">
	        <field name="name">res.config.settings.inherit.view.form</field>
	        <field name="model">res.config.settings</field>
	        <field name="priority" eval="100"/>
	        <field name="inherit_id" ref="base.res_config_settings_view_form"/>
	        <field name="arch" type="xml">
		        <xpath expr="//div[@id='invoicing_settings']" position="after">
			        <h2>Retenciones</h2>
			        <div class="col-xs-12 col-md-6 o_setting_box" id="retention_iva">
	                    <div class="o_setting_left_pane">
		                    <span class="o_form_label">Cuenta de Retenciones de IVA</span>
	                        <div class="text-muted">
	                            Ingrese la cuenta contable para las retenciones de IVA de proveedor
	                        </div>
	                    </div>
				        <div class="o_setting_right_pane">
                            <div class="row mt16">
                                <label for="account_retention_iva" class="col-lg-3 o_light_label"/>
                                <field name="account_retention_iva" required="1" options="{'no_create_edit': True, 'no_open': True}"/>
                            </div>
	                    </div>
	                </div>

			        <div class="col-xs-12 col-md-6 o_setting_box" id="retention_islr">
				        <div class="o_setting_left_pane">
	                        <span class="o_form_label">Cuenta de Retenciones de ISLR</span>
	                        <div class="text-muted">
	                            Ingrese la cuenta contable para las retenciones de ISLR de proveedor
	                        </div>
	                    </div>
	                    <div class="o_setting_right_pane">
                            <div class="row mt16">
                                <label for="account_retention_islr" class="col-lg-3 o_light_label"/>
                                <field name="account_retention_islr" required="1" options="{'no_create_edit': True, 'no_open': True}"/>
                            </div>
	                    </div>
	                </div>

			        <div class="col-xs-12 col-md-6 o_setting_box" id="retention_iva">
	                    <div class="o_setting_left_pane">
		                    <span class="o_form_label">Diario de Retenciones de Clientes</span>
	                        <div class="text-muted">
	                            Ingrese el diario para las retenciones de cliente
	                        </div>
	                    </div>
				        <div class="o_setting_right_pane">
                            <div class="row mt16">
                                <label for="journal_retention_client" class="col-lg-3 o_light_label"/>
                                <field name="journal_retention_client" required="1" options="{'no_create_edit': True, 'no_open': True}"/>
                            </div>
	                    </div>
	                </div>

			        <div class="col-xs-12 col-md-6 o_setting_box" id="retention_iva">
	                    <div class="o_setting_left_pane">
		                    <span class="o_form_label">Diario de Retenciones de Proveedor</span>
	                        <div class="text-muted">
	                            Ingrese el diario para las retenciones de proveedor
	                        </div>
	                    </div>
				        <div class="o_setting_right_pane">
                            <div class="row mt16">
                                <label for="journal_retention_supplier" class="col-lg-3 o_light_label"/>
                                <field name="journal_retention_supplier" required="1" options="{'no_create_edit': True, 'no_open': True}"/>
                            </div>
	                    </div>
	                </div>

			        <div class="col-xs-12 col-md-6 o_setting_box" id="retention_iva">
	                    <div class="o_setting_left_pane">
		                    <span class="o_form_label">Cuenta P/cobrar clientes</span>
	                        <div class="text-muted">
	                            Ingrese cuenta por cobrar de retenciones de clientes
	                        </div>
	                    </div>
				        <div class="o_setting_right_pane">
                            <div class="row mt16">
                                <label for="account_retention_receivable_client" class="col-lg-3 o_light_label"/>
                                <field name="account_retention_receivable_client" required="1" options="{'no_create_edit': True, 'no_open': True}"/>
                            </div>
	                    </div>
	                </div>

			        <div class="col-xs-12 col-md-6 o_setting_box" id="retention_iva">
	                    <div class="o_setting_left_pane">
		                    <span class="o_form_label">Cuenta P/pagar proveedor</span>
	                        <div class="text-muted">
	                            Ingrese cuenta por pagar de retenciones de proveedores
	                        </div>
	                    </div>
				        <div class="o_setting_right_pane">
                            <div class="row mt16">
                                <label for="account_retention_to_pay_supplier" class="col-lg-3 o_light_label"/>
                                <field name="account_retention_to_pay_supplier" required="1" options="{'no_create_edit': True, 'no_open': True}"/>
                            </div>
	                    </div>
	                </div>
				</xpath>
	        </field>
	  </record>
  </data>
</odoo>