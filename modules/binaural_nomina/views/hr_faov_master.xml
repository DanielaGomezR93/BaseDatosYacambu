<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="binaural_faov_master_view_form" model="ir.ui.view">
        <field name="name">Master Faov</field>
        <field name="model">hr.faov.master</field>
        <field name="arch" type="xml">
            <form string="Maestro FAOV">
                <sheet>
                    <group>
                        <field name="porcentaje_deduccion" />                        
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="hr_faov_master_action" model="ir.actions.act_window">
        <field name="name">Maestro FAOV</field>
        <field name="res_model">hr.faov.master</field>
        <field name="view_mode">tree,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>        
    </record>

</odoo>
