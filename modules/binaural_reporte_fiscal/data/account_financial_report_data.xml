<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="action_account_report_balance_comprobacion" model="ir.actions.client">
            <field name="name">Balance de comprobaci√≥n</field>
            <field name="tag">account_report</field>
            <field name="context" eval="{'model': 'account.checking.balance'}" />
        </record>

	<!-- Accciones de Reportes en USD -->
        <record id="action_account_report_ar_usd" model="ir.actions.client">
            <field name="name">Vencida por Cobrar $</field>
            <field name="tag">account_report</field>
            <field name="context" eval="{'model': 'account.aged.receivable', 'USD': True}" />
        </record>
        <record id="action_account_report_ap_usd" model="ir.actions.client">
            <field name="name">Vencida por Pagar $</field>
            <field name="tag">account_report</field>
            <field name="context" eval="{'model': 'account.aged.payable', 'USD': True}" />
        </record>

	<!-- Menuitems de Reportes en USD -->
	<menuitem id="menu_action_account_report_aged_receivable_usd"
		  name="Vencida por Cobrar $"
		  action="action_account_report_ar_usd"
		  parent="account_reports.account_reports_partners_reports_menu"/>
	<menuitem id="menu_action_account_report_aged_payable_usd"
		  name="Vencida por Pagar $"
		  action="action_account_report_ap_usd"
		  parent="account_reports.account_reports_partners_reports_menu"/>
